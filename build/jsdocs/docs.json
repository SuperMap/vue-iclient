
{
    "opts": {
        "recurse": true,
        "template": "./template",
        "configPath": "./",
        "destination": "./docs"
    },
    "disqus":"",
    "tags": {
        "allowUnknownTags": true
    },
    "i18n": {
        "defaultLang": "en",
        "possibleLang": ["en"]
    },
    "source": {
        "includePattern": ".+\\.(vue|js)$",
        
        // "excludePattern": ".+\\\\src\\\\common\\\\iServer\\\\.*Service.js?$",
        "include": [
            // "src/view/components"
             "src"
        ],
        "exclude": [
            // "src/common/overlay/levelRenderer"
        ]
        
    },
    "plugins": ["plugins/markdown","./plugins/factoryFunction","node_modules/jsdoc-vuejs"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": false
        }
        //"applicationName": "iClient for Leaflet"
    }
}
