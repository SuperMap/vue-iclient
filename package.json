{
  "name": "@supermapgis/vue-iclient",
  "version": "12.0.0-dev",
  "description": "SuperMap iClient for Vue.js",
  "homepage": "https://iclient.supermap.io",
  "workspaces": [
    "vue2",
    "vue3"
  ],
  "scripts": {
    "dev:vue2": "npm run dev --workspace=vue2",
    "build:vue2": "npm run build --workspace=vue2",
    "lib:vue2": "npm run lib --workspace=vue2",
    "lib:vue2:mapboxgl": "npm run lib:mapboxgl --workspace=vue2",
    "lib:vue2:leaflet": "npm run lib:leaflet --workspace=vue2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "author": "SuperMap_FE",
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperMap/vue-iclient.git"
  },
  "keywords": [
    "SuperMap",
    "Mapbox GL",
    "Vue"
  ],
  "jestSonar": {
    "reportPath": "test/unit/report",
    "reportFile": "test-reporter.xml",
    "indent": 4,
    "reportedFilePath": "relative"
  },
  "engines": {
    "npm": ">=7.0.0"
  },
  "license": "Apache-2.0"
}
