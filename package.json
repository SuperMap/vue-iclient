{
  "name": "@supermapgis/vue-iclient",
  "version": "12.0.0-dev",
  "description": "SuperMap iClient for Vue.js",
  "homepage": "https://iclient.supermap.io",
  "scripts": {
    "install:vue2": "npm install --legacy-peer-deps --prefix vue2",
    "dev:vue2": "npm run dev --prefix vue2 --",
    "build:vue2": "npm run build --prefix vue2 --",
    "docs:dev:vue2": "npm run docs:dev --prefix vue2",
    "docs:build:vue2": "npm run docs:build --prefix vue2",
    "storybook:dev:vue2": "npm run storybook:dev --prefix vue2",
    "storybook:build:vue2": "npm run storybook:build --prefix vue2",
    "lib:vue2": "npm run lib --prefix vue2",
    "lib:mapboxgl:vue2": "npm run lib:mapboxgl --prefix vue2",
    "lib:leaflet:vue2": "npm run lib:leaflet --prefix vue2",
    "publish:all:vue2": "npm run publish:all --prefix vue2 --",
    "publish:mapboxgl:vue2": "npm run publish:mapboxgl --prefix vue2 --",
    "publish:leaflet:vue2": "npm run publish:leaflet --prefix vue2 --",
    "unit:vue2": "npm run unit --prefix vue2",
    "test:vue2": "npm run test --prefix vue2",
    "lint:vue2": "npm run lint --prefix vue2",
    "lint:fix:vue2": "npm run lint:fix --prefix vue2",
    "prepare": "husky install",
    "dev:vue3": "pnpm --dir ./vue3 dev",
    "typecheck:vue3": "pnpm --dir ./vue3 typecheck:web",
    "build-mapboxgl:vue3": "pnpm --dir ./vue3 build-mapboxgl",
    "publish-mapboxgl:vue3": "pnpm publish ./vue3/dist/vue-iclient-mapboxgl --registry http://localhost:4873 --no-git-checks"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "author": "SuperMap_FE",
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperMap/vue-iclient.git"
  },
  "keywords": [
    "SuperMap",
    "Mapbox GL",
    "Vue"
  ],
  "jestSonar": {
    "reportPath": "test/unit/report",
    "reportFile": "test-reporter.xml",
    "indent": 4,
    "reportedFilePath": "relative"
  },
  "engines": {
    "npm": ">=7.0.0"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "husky": "^8.0.0",
    "lint-staged": "^15.4.3",
    "vue-iclient-core": "workspace:*"
  },
  "lint-staged": {
    "vue3/**/*.{vue,ts,tsx}": "pnpm typecheck:vue3",
    "vue3/**/*.{vue,js,ts,jsx,tsx,md,json}": "pnpm --dir ./vue3 lint"
  }
}
