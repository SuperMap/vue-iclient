{
  "name": "vue3",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/antd.min.js",
  "typings": "es/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "typings/global.d.ts",
    "locale"
  ],
  "scripts": {
    "test:unit": "vitest",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "dev-mapboxgl:site": "pnpm --dir ./site dev",
    "build-mapboxgl:site": "pnpm --dir ./site build",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "build-mapboxgl": "pnpm --dir build-packages/build start --pkg mapboxgl",
    "build-mapboxgl:theme": "pnpm --dir packages/mapboxgl/theme-chalk build",
    "stub": "pnpm run -r --parallel stub",
    "typecheck": "run-p typecheck:web typecheck:node",
    "typecheck:web": "vue-tsc -p tsconfig.web.json --composite false --noEmit",
    "typecheck:node": "tsc -p tsconfig.node.json --noEmit"
  },
  "dependencies": {
    "@supermapgis/mapboxgl/theme-chalk": "workspace:*",
    "@supermapgis/vue-iclient-mapboxgl": "workspace:*",
    "vue": "3.5.12"
  },
  "devDependencies": {
    "@pnpm/find-workspace-packages": "^4.0.16",
    "@supermapgis/build": "workspace:*",
    "@supermapgis/build-utils": "workspace:*",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.20",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/runtime-core": "3.5.12",
    "@vue/runtime-dom": "3.5.12",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.83.4",
    "typescript": "~5.6.3",
    "unplugin-vue-macros": "^2.7.11",
    "vite": "^6.0.5",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  }
}
