{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "target": "es2018",
    "module": "esnext",
    "sourceMap": false,
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": true,
    "strict": false,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "removeComments": false,
    "noImplicitAny": false,
    "pretty": true,
    "preserveSymlinks": true,
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "paths": {
      "*": [
        "node_modules/*",
        ".pnpm/*",
      ],
      "@supermapgis/": [
        "packages/"
      ],
      "vue-iclient-core/": [
        "../core/"
      ]
    },
    "types": [
      "mapbox-gl",
      "vue",
      "@vue/runtime-dom",
      "geojson"
    ]
  }
}